#!/usr/bin/env bash

if [[ -z "$_HUB_CS" ]]; then
    echo "Error: the _HUB_CS environment variable is not defined."
    exit -1
fi

if [[ -z "$_EH_CS" ]]; then
    echo "Error: the _EH_CS environment variable is not defined."
    exit -1
fi

if [[ -z "$_STORE_CS" ]]; then
    echo "Error: the _STORE_CS environment variable is not defined."
    exit -1
fi
